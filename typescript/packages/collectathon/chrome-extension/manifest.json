{
  "manifest_version": 3,
  "name": "Collectathon Clipper",
  "version": "1.0",
  "description": "Clip URLs and selected content to Collectathon",
  "permissions": [
    "activeTab",
    "contextMenus",
    "storage"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  }
}
